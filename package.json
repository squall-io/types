{
  "name": "@squall.io/types",
  "version": "0.0.3",
  "description": "Utility types to extends TypeScript capabilities",
  "main": "index.d.ts",
  "repository": "https://github.io/squall-io/types",
  "author": "Salathiel Genese <salathiel@genese.name>",
  "license": "MIT",
  "types": "./index.d.ts",
  "scripts": {
    "prepack": "ts-node build.ts",
    "build": "ts-node build.ts",
    "dev:build": "nodemon --exec 'ts-node build.ts'"
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [
      "./src",
      "./build.ts"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.6.2",
    "nodemon": "^2.0.4",
    "replace-in-files": "^2.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
